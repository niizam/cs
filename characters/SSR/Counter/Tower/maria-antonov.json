            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Maria is a support/damage dealer type Tower that doesn't move from the spot she was summoned and summons Mech allies to help the team instead. Which makes her good on both auto and manual play, because she will always do something. Her passive buff is one of the strongest damage amplifiers in the game, which makes her a great choice for Raid teams.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVP she forces the enemy's hand to rush her or risk losing the game by allowing her to constantly nuke the whole battlefield with her special and summoning an army of mechs on her ultimate. Still, because A. Na Yubin exists like all Towers, if you want to use Maria, you have to be wary of him since he's a perfect counter.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- great passive buff to everyone,\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- her special skill hurts and can hit whole battlefield.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- gear hungry; she really wants a 72% CDR set,\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- A. Na Yubin exists.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
