            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Grendel is a long range backline damage dealer with a well-rounded kit. Grendel is notable in that it has (what is effectively) mapwide barrier reflux + barrier nullification, applied regularly through its enhanced basic attack after Special Skill.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVE, Grendel occupies the competitive dedicated damage dealer niche. Grendel was never particularly good, but in the current state of the game it has been thoroughly powercrept out of the meta. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVP, Grendel sees infrequent play in mech comps as a damage dealer on account of its synergy with Coffin-6, otherwise it has largely disappeared from the meta in favor of stronger alternatives.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Extremely long range barrier reflux + barrier nullification with decent uptime. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- Special and lower hitstun immunity allows Grendel to maintain a constant steady damage output by ignoring the effects of most types of hitstun. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"nodeType\":\"document\",\"data\":{},\"content\":[{\"nodeType\":\"paragraph\",\"data\":{},\"content\":[{\"nodeType\":\"text\",\"value\":\"- \",\"marks\":[],\"data\":{}},{\"nodeType\":\"text\",\"value\":\"Overshadowed by stronger dedicated damage dealers. \",\"marks\":[],\"data\":{}}]}]}"
