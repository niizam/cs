            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Terminator is one of the most-buffed employees in the game and his current version makes him one of the best PVE oriented strikers in the game. Not only his damage is pretty damn good, but he also can debuff enemies' DEF which is a great thing to have especially on bosses - and that's where he shines the most. Sadly he's really squishy and you need to have a defender in front of him or else he will die without doing anything.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- DEF reduction is one of the best debuffs in the game,\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- great damage.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
