            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Orca gains bonus damage against C.O.s, which makes her a great PVE employee. Before her rework, she needed to kill enemies herself to activate the healing and ATK bonus from her passive, but now she just needs to hit an enemy within 4 seconds of his/her death. Her ultimate also deals True Damage and can be reset on kill, but in harder PVE content where enemies' health pools are massive, this rarely gets activated. Also, for some reason, the Replacers in story mode are not registered as Replacer types so she does not get her bonus against them.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Overall, she is a good pure damage dealer in PVE, but in a lot of content, you have to choose your team wisely so other Strikers that also can do damage while helping the team with debuffs/buffs are often picked instead of her.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- can self-sustain pretty well thanks to the heal on her passive,\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- true damage on her ult deals pretty nice damage.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- useless in PVP,\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- lacks utility to make her worth using over other Strikers.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
