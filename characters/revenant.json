            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Revenant is a PVP-oriented backline supporter designed as a dedicated buffer for multi-unit soldier teams. Revenant addresses multi-unit soldiers’ long-standing survivability issues with constant barrier application and further amplifies their already high damage output. The viability of multi-unit soldier teams is entirely attributed to Revenant.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVE, Revenant has seen historical use in Dimension Trimming Swift Room 2 and Dimension Trimming Volcano Room 3. Nowadays, there exist better setups for both stages, rendering Revenant-based comps largely obsolete.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVP, Revenant struggles to keep the increasingly outdated multi-unit soldiers relevant. As new releases push damage and survivability across all other comps, the long-standing issues inherent to multi-unit soldier comps are becoming too difficult to ignore.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Extremely powerful support for multi-unit soldiers.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"nodeType\":\"document\",\"data\":{},\"content\":[{\"nodeType\":\"paragraph\",\"data\":{},\"content\":[{\"nodeType\":\"text\",\"value\":\"- \",\"marks\":[],\"data\":{}},{\"nodeType\":\"text\",\"value\":\"The last multi unit-soldier that benefited from Revenant was released over 2 years ago.\",\"marks\":[],\"data\":{}}]}]}"
