            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Same old Kang, but now better. Snipers tremble in her presence, praying for evasion RNG. Not only is her damage simply higher than before, she now has a skill that finishes every sniper that somehow survived a car accident. If miracle happens and you survived both - congrats you are now blind, skill sealed and atk debuffed for 5 seconds. Also sees a lot of use in terror comps as cheap and effective way to just throw DP at enemy ship for damage.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Her PvE use didn't really change, she is still just a buffer for soldier comps. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- can easily kill most Snipers in the game,\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- if she can't kill them instantly, she will Seal their skills and debuff them.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- Fury users aren't that good yet and require full ASPD gear to work.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "newRatings": {
