            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Administration Rifleman is one of the best Rangers in the game. For a 2-cost employee, they deal absurd amounts of damage, leading to them being used in both PVP and PVE in various teams, even mixed ones with Counters. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"They do have one major weakness though - they're easy to kill. In PVP if they get hit by a wild AOE or Ingrid gets dropped behind them, they just die.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Due to their low cost, they are one (two?) of the best employees for Shadow Palace and other modes that put a cap on Deployment Resources.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- great damage for his 2 cost.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
