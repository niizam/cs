            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Liv Allen is a Striker that sacrifices her ability to do damage to provide support to her allies and to control enemies instead. Her passive gives Role ADV DMG Res, which sounds great, but in reality, the 30% translates only to like 10% less damage taken, because it is multiplicative, not additive with all the other sources of DMG Res. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"This makes her pretty useless in both PVE and PVP (although when she is an upped employee, some players still use her).\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- supports her allies with buffs.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
