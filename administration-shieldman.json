            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Old Administration Shieldman (Admin Shieldman) is a multi-unit frontline tank with mediocre stats. Their one redeeming quality is that they are decently tanky for how cheap they are. Otherwise, as a low rarity unit, Admin Shieldman does not have the stats necessary to meet the baseline requirements expected of them as a frontline tank.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVE, Admin Shieldman sees use as a core unit in Britra raid setups. As part of raid mechanics, a soldier frontline tank is needed to tank damage for the first minute of the raid. Admin Shieldman is the cheapest good soldier frontline in the game.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Otherwise, Admin Shieldman struggles to distinguish himself from the pool of stronger, cheaper alternatives (e.g. Rearm Kaci, Rearm Alex).\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- Cheap.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"nodeType\":\"document\",\"data\":{},\"content\":[{\"nodeType\":\"paragraph\",\"data\":{},\"content\":[{\"nodeType\":\"text\",\"value\":\"- For a frontline tank whose sole redeeming quality is cost, they aren't that cost-efficient.\",\"marks\":[],\"data\":{}}]},{\"nodeType\":\"paragraph\",\"data\":{},\"content\":[{\"nodeType\":\"text\",\"value\":\"- Poor EVA.\",\"marks\":[],\"data\":{}}]}]}"
