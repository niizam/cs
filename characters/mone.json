            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Mone is a PVE-oriented utility tank with a niche skill sealing enemies vulnerable to skill seal debuffs. Properly geared, her passive is capable of providing near-permanent uptime on skill seal. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVE, Mone is primarily used as part of a strategy to trivialize the Guild Coop Demolition Worm boss levels 1 ~ 6. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Mone is held back by her lackluster stats. As a low rarity unit, Mone does not have the stats necessary to meet the baseline requirements expected of her as a frontline tank. Permanent uptime skill seal is nice to have, but it can be a challenge keeping Mone alive to apply it.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Highest uptime skill seal in the game. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"nodeType\":\"document\",\"data\":{},\"content\":[{\"nodeType\":\"paragraph\",\"data\":{},\"content\":[{\"nodeType\":\"text\",\"value\":\"- \",\"marks\":[],\"data\":{}},{\"nodeType\":\"text\",\"value\":\"Most content is immune to skill seal. \",\"marks\":[],\"data\":{}}]}]}"
