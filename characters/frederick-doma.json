            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Frederick Doma is a backline utility damage dealer typically geared as a defender shredder. Doma’s kit is notable in that it comes with DMG Taken and Incoming Healing debuffs, giving him some utility to push teamwide damage.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Doma’s \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[{\"type\":\"bold\"}],\"value\":\"Alternium Reactor \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"provides a significant boost to his damage, putting him on the same level as his peers. For reference, Doma’s reactor puts his damage on par with Rearm Gaeun (RGaeun), the benchmark for SSR ranger performance.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVP, Doma may be played as an alternative to RGaeun as a dedicated damage dealer to shred defenders. As RGaeun herself gets phased out of the meta (it’s complicated), so too is the traditional approach to team building. This means characters like Doma who occupy the same niche join her on the bench.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Applies offensive debuffs to enemy targets.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Outperformed by newer, stronger dedicated damage dealers.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
