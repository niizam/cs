            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Benedict is a defender, that can't really tank. He doesn't have a Taunt and without MDL he's often used as a sub-defender that CC enemies. Sadly, there are dozens of 3-cost employees that have excellent CC abilities and also do other things (like damage or debuff enemies), so Benedict is currently quite useless.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- his special has quite a big range allowing him to stun multiple enemies.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
