            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Nanahara Chinatsu is a backline supporter who provides healing for the team. Compared to other healers, Chinatsu trades away some healing% in exchange for (among other things) a DMG RDC% buff. This puts her behind her peers (i.e. Evelyn, Claudia) in raw healing output. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"As supporters’ personal stats don’t factor into the final buff values (buffs scale with target stats), stat scaling by rarity is largely secondary to the potency of the buffs/ debuffs they bring to the team.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Chinatsu struggles to compete with other healers. The DMG RDC% buff is not enough to compensate for her diminished healing. There are few scenarios where Chinatsu would be a noticeable improvement over the alternatives.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"None of Chinatsu’s flaws matter because leveling her is a prerequisite for Rearm Chinatsu, the most broken healer in the game. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"nodeType\":\"document\",\"data\":{},\"content\":[{\"nodeType\":\"paragraph\",\"data\":{},\"content\":[{\"nodeType\":\"text\",\"value\":\"- \",\"marks\":[],\"data\":{}},{\"nodeType\":\"text\",\"value\":\"Chinatsu, at base, is the worst sustain support in the game.\",\"marks\":[],\"data\":{}}]}]}"
