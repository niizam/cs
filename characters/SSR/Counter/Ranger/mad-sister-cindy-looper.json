            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Mad Sister Cindy Looper is a PVP-oriented long range backline damage dealer notable for her mapwide Ultimate skill that boomerangs through the enemy team. Importantly, the Ultimate skill projectile is capable of “bypassing” valid hit absorption on the return, as it flies from back-to-front to hit the backline first.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVP, Cindy is played for her backline-reaching Ultimate skill, which, in the best-case-scenario, can seriously chunk backline squishies (assuming the attack hits). Repeated casts of Cindy’s ult can quickly shutdown enemy backlines with the pressure it creates. Cindy does not have sufficient damage to justify her cost as a single-target ranger damage dealer, and does not have a use beyond her backline access damage potential.  \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Latest in novelty backline access units. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- Cindy is exceptionally squishy for a high cost ranger.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- Cindy does not have iframes for the full ultimate cast animation, meaning she can be disrupted out her ult.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- Still ultimately a novelty backline access unit.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
