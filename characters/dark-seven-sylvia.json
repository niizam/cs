            "reviewGeneral": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"Dark Seven Sylvia Lena Cooper (RSylvia) is a PVP-oriented backline access damage dealer. At the core of RSylvia’s kit is her extremely long-range Special skill, which fires off an AOE DOT field at the furthest unit in range. The Special skill also increases skill cooldowns against targets hit, providing some limited utility beyond damage. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"},{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"In PVP, RSylvia sees limited to no play owing to her low impact for how much she costs. RSylvia has become increasingly ineffective within her niche as her chip damage is shrugged off by enemy healers. For a unit designed around shutting down backline units, RSylvia’s insufficient early pressure doesn’t do nearly enough to influence the game in any meaningful way.\",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "pros": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Long range repeated backline hate. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
            },
            "cons": {
              "raw": "{\"data\":{},\"content\":[{\"data\":{},\"content\":[{\"data\":{},\"marks\":[],\"value\":\"- \",\"nodeType\":\"text\"},{\"data\":{},\"marks\":[],\"value\":\"Lacks damage for cost. \",\"nodeType\":\"text\"}],\"nodeType\":\"paragraph\"}],\"nodeType\":\"document\"}"
